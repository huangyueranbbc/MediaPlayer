<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#00ffffff" android:endColor="#00808a87" android:angle="270.0" android:centerY="0.45" android:centerColor="#ffffffff" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#00ffffff" android:endColor="#00aabd00" android:angle="270.0" android:centerY="0.45" android:centerColor="#ffffff00" />
            </shape>
        </clip>
    </item>
</layer-list>